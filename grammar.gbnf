root ::= (" "| "\n") grammar-models
grammar-models ::= relations
relations ::= "{"  ws "\"relations\"" ": " relations-relations  ws "}"
relation ::= "{"  ws "\"link_type\"" ": " relation-link-type ","  ws "\"subject_text_span\"" ": " string ","  ws "\"subject_location\"" ": " relation-subject-location ","  ws "\"object_text_span\"" ": " string ","  ws "\"object_location\"" ": " relation-object-location  ws "}"
relation-link-type ::= "\"anatomical location | located in | human\"" | "\"anatomical location | located in | animal\"" | "\"bacteria | interact | bacteria\"" | "\"bacteria | interact | chemical\"" | "\"bacteria | interact | drug\"" | "\"bacteria | influence | DDF\"" | "\"bacteria | change expression | gene\"" | "\"bacteria | located in | human\"" | "\"bacteria | located in | animal\"" | "\"bacteria | part of | microbiome\"" | "\"chemical | located in | anatomical location\"" | "\"chemical | located in | human\"" | "\"chemical | located in | animal\"" | "\"chemical | interact | chemical\"" | "\"chemical | part of | chemical\"" | "\"chemical | impact | microbiome\"" | "\"chemical | produced by | microbiome\"" | "\"chemical | impact | bacteria\"" | "\"dietary supplement | impact | bacteria\"" | "\"dietary supplement | impact | microbiome\"" | "\"drug | impact | bacteria\"" | "\"drug | impact | microbiome\"" | "\"food | impact | bacteria\"" | "\"food | impact | microbiome\"" | "\"chemical | influence | DDF\"" | "\"dietary supplement | influence | DDF\"" | "\"drug | influence | DDF\"" | "\"food | influence | DDF\"" | "\"chemical | change expression | gene\"" | "\"dietary supplement | change expression | gene\"" | "\"drug | change expression | gene\"" | "\"food | change expression | gene\"" | "\"chemical | administered | human\"" | "\"chemical | administered | animal\"" | "\"dietary supplement | administered | human\"" | "\"dietary supplement | administered | animal\"" | "\"drug | administered | human\"" | "\"drug | administered | animal\"" | "\"food | administered | human\"" | "\"food | administered | animal\"" | "\"DDF | strike | anatomical location\"" | "\"DDF | change abundance | bacteria\"" | "\"DDF | change abundance | microbiome\"" | "\"DDF | affect | DDF\"" | "\"DDF | is a | DDF\"" | "\"DDF | target | human\"" | "\"DDF | target | animal\"" | "\"drug | interact | chemical\"" | "\"drug | interact | drug\"" | "\"DDF | interact | chemical\"" | "\"DDF | interact | drug\"" | "\"drug | change effect | DDF\"" | "\"microbiome | located in | anatomical location\"" | "\"microbiome | located in | human\"" | "\"microbiome | located in | animal\"" | "\"microbiome | used by | biomedical technique\"" | "\"human | used by | biomedical technique\"" | "\"microbiome | change expression | gene\"" | "\"microbiome | is linked to | DDF\"" | "\"microbiome | compared to | microbiome\""
relation-subject-location ::= "\"abstract\"" | "\"text\"" | "\"title\""
relation-object-location ::= "\"abstract\"" | "\"text\"" | "\"title\""
relations-relations ::= "[" ws (relation)? ("," ws relation)* ws "]" 

boolean ::= "true" | "false"
null ::= "null"
string ::= "\"" (
        [^"\\] |
        "\\" (["\\/bfnrt] | "u" [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F])
      )* "\""
ws ::= ([ \t\n]+)
number ::= "-"? ([0-9]+ | [0-9]+ "." [0-9]+) ([eE] [-+]? [0-9]+)?